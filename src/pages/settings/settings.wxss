.person {
	width: 100%;
	height: 45px;
}
.setting_list {
	width: 96%;
	height: 45px;
	float: right;
	border-bottom: 1px solid rgba(173,185,193,0.5);
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 16px;
}
.list_right {
	margin-right: 10px;
	color: rgba(173,185,193,0.5);
}
.setting_list switch {
	margin:2px 10px 0 0;
}
.list_end {
	margin-bottom: 20px;
}
button {
	position: absolute;
	bottom: 120px;
	right: 0;
	left:0;
	display: block;
	width: 94%;
	height: 46px;
	border-radius: 0;
}


.chatRoom_tab {
	width: 100%;
	height: 50px;
	background-color: rgb(250,251,252);
	position: fixed;
	bottom: 0;
	right: 0;
	border-top: 1px solid rgba(173,185,193,0.5);
	display: flex;
	justify-content: space-around;
}
.contacts,.settings {
	width: 47px;
	height: 37px;
	margin-top: 8px;
}
.chats {
	width: 35px;
	height: 37px;
	margin-top: 8px;
}
.contacts image,.settings image {
	width: 100%;
	height: 100%;
}
.chats image {
	width: 100%;
	height: 100%;
}